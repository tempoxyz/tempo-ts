{
	"name": "tempo",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"build": "zshy --project ../tsconfig.build.json",
		"postinstall": "bun run build"
	},
	"zshy": {
		"exports": {
			".": "./index.ts",
			"./ox": "./ox/index.ts",
			"./prool": "./prool/index.ts"
		},
		"cjs": false,
		"conditions": {
			"src": "src"
		}
	},
	"peerDependencies": {
		"ox": "^0",
		"prool": "^0"
	},
	"peerDependenciesMeta": {
		"ox": {
			"optional": true
		},
		"prool": {
			"optional": true
		}
	},
	"author": "jxom <j@wevm.dev>",
	"license": "MIT",
	"files": [
		"dist"
	],
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"src": "./index.ts",
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./ox": {
			"src": "./ox/index.ts",
			"types": "./dist/ox/index.d.ts",
			"import": "./dist/ox/index.js"
		},
		"./prool": {
			"src": "./prool/index.ts",
			"types": "./dist/prool/index.d.ts",
			"import": "./dist/prool/index.js"
		}
	}
}
